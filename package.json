{
  "name": "codeflow-commander",
  "version": "1.0.0",
  "description": "Enterprise-grade CI/CD simulator with AI code review, compliance validation, and security governance",
  "private": true,
  "type": "module",
  "workspaces": [
    "backend",
    "cli-tool"
  ],
  "scripts": {
    "install-all": "npm install && npm run install:workspaces",
    "install:workspaces": "npm run --workspace backend install && npm run --workspace cli-tool install",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm run --workspace backend dev",
    "dev:cli": "npm run --workspace cli-tool dev",
    "clean": "npm run clean --workspaces",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "ci-cd",
    "ai",
    "code-review",
    "compliance",
    "security",
    "enterprise"
  ],
  "author": "CodeFlow Team",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
